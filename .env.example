MODEL_ROUTING_DEFAULT_MODEL=gpt-4o-mini
MODEL_PRICE_DEFAULT_INPUT_USD=0.0000015
MODEL_PRICE_DEFAULT_OUTPUT_USD=0.000003

# Backend runtime mode
# - single_process (default): no external coordination required
# - distributed: requires shared coordination + (eventually) shared persistence
BACKEND_MODE=single_process
REDIS_URL=redis://localhost:6379/0
RUN_LEASE_TTL_SECONDS=30
TOOL_QUEUE_CONSUMER_NAME=

RATE_LIMIT_GLOBAL_CONCURRENCY=8
RATE_LIMIT_TENANT_CONCURRENCY=4
RUN_MODEL_BUDGET_USD=0.05
CACHE_RETRIEVAL_ENABLED=1
CACHE_TOOL_RESULTS_ENABLED=1

# Optional but recommended
OPENAI_API_KEY=replace-me

# Frontend
NEXT_PUBLIC_BACKEND_URL=http://localhost:8000
NEXT_PUBLIC_TENANT_ID=tenant-demo
NEXT_PUBLIC_USER_ID=user-demo
